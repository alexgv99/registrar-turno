{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
			"no-tabs": "off",
			"indent": ["error", "tab"],
			"arrow-parens": ["error", "as-needed"],
			"comma-dangle": ["error", "never"],
			"no-console": [
				"error",
				{
								"allow": [
												"debug",
												"error",
												"time",
												"timeEnd"
								]
				}
		],
		"import/extensions": "off"
	}
}
